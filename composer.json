{
    "name": "misfist/misfist-packages",
    "description": "WordPress packages",
    "type": "project",
    "license": "GLP-3.0",
    "authors": [
        {
            "name": "Pea",
            "email": "pea@misfist.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "misfist/advanced-custom-fields-pro",
                "version": "5.6.3",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/advanced-custom-fields/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/misfist/advanced-custom-fields.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "misfist/admin-columns-pro",
                "version": "master",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/admin-columns/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/misfist/admin-columns.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "misfist/misfist",
                "version": "master",
                "type": "wordpress-theme",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/misfist-theme/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/misfist/misfist-theme.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "misfist/admin-columns-pro": "dev-master",
        "misfist/advanced-custom-fields-pro": "^5.6.3",
        "wpackagist-plugin/jetpack": "5.8",
        "wpackagist-theme/understrap": "0.8.0",
        "misfist/misfist": "dev-master"
    },
    "require-dev": {
      "wpackagist-plugin/developer": "1.2.6",
      "wpackagist-plugin/debug-bar-console": "0.3",
      "wpackagist-plugin/which-template": "4.0"
    }
}
